Object subclass: #MyOrganization	instanceVariableNames: 'rawData'	classVariableNames: ''	package: 'MyPackage'!!MyOrganization methodsFor: 'accessing' stamp: 'GlamorousAuthor 12/20/2022 09:40'!rawData: dictionary	rawData := dictionary! !!MyOrganization methodsFor: 'accessing' stamp: 'GlamorousAuthor 12/20/2022 09:57'!repos	| json url |	url := rawData at: 'repos_url'.	json := ZnClient new get: url.	^ (STON fromString: json)			collect: [ :each | MyRepository new rawData: each ]! !!MyOrganization methodsFor: 'accessing' stamp: 'GlamorousAuthor 12/20/2022 10:02'!gtReposFor: aView	<gtView>	^ aView list		title: 'Repositories';		priority: 10;		items: [ self repos ];		itemText: [: each | each fullName ]! !Object subclass: #MyRepository	instanceVariableNames: 'rawData'	classVariableNames: ''	package: 'MyPackage'!!MyRepository methodsFor: 'accessing' stamp: 'GlamorousAuthor 12/20/2022 09:57'!rawData: dictionary	rawData := dictionary! !!MyRepository methodsFor: 'accessing' stamp: 'GlamorousAuthor 12/20/2022 10:00'!gtPropertiesFor: aView	<gtView>	^ aView forward		title: 'Properties';		priority: 10;		object: [ rawData ];		view: #gtItemsFor:! !!MyRepository methodsFor: 'accessing' stamp: 'GlamorousAuthor 12/20/2022 10:01'!fullName	^ rawData at: 'full_name'! !